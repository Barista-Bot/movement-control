<launch>

  <!-- TRACKING INFO FROM THE XTION -->
  <node pkg="openni_tracker" type="openni_tracker" name="openni_tracker" required="true" output="screen" />

  <!-- p2os driver (moving the P3-AT) -->
  <!--<node pkg="p2os_driver" type="p2os_driver" name="p2os" required="true" output="screen">
    <rosparam>
      port : "/dev/ttyUSB0"
      pulse : 1
      use_sonar : true
    </rosparam>
  </node>-->

  <node pkg="movement" type="movement_alt.py" name ="movement" required="true" output="screen">
    <param name="noxtion" value="0" />
    <remap from="cmd_vel" to="xt_cmd_vel" />
  </node>
  <node pkg="movement" type="joystick_node.py" name="joystick_node" required="true" output="screen" />
  <node pkg="movement" type="joystick_to_cmd_vel.py" name="joystick_to_cmd_vel" required="true"  output="screen">
    <remap from="cmd_vel" to="js_cmd_vel" />
  </node>
  <node pkg="movement" type="transform.py" name="movementtransform" required="true" output="screen" />

  <node pkg="voice_control" type="voice_control_server.py" name="voice_control" required="true" output="screen" />

  <!--<node pkg="user_identification" type="user_identification_server.py" name="user_identification_server" required="true" output="screen" />-->

</launch>
